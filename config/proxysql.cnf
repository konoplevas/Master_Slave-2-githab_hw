datadir="/var/lib/proxysql"

admin_variables=
{
admin_credentials="admin:admin"
mysql_ifaces="0.0.0.0:6032"
}

mysql_variables=
{
threads=4
max_connections=2048
default_query_delay=0
default_query_timeout=36000000
have_compress=true
poll_timeout=2000
interfaces="0.0.0.0:6033"
default_schema="information_schema"
stacksize=1048576
server_version="5.5.30"
connect_timeout_server=3000
monitor_username="monitor"
monitor_password="monitor"
monitor_history=600000
monitor_connect_interval=60000
monitor_ping_interval=10000
monitor_read_only_interval=1500
monitor_read_only_timeout=500
ping_interval_server_msec=120000
ping_timeout_server=500
commands_stats=true
sessions_sort=true
connect_retries_on_failure=10
}

mysql_servers =
(
{
address="mysql-shard1-master"
port=3306
hostgroup=10
max_connections=100
max_replication_lag=10
},
{
address="mysql-shard1-slave"
port=3306
hostgroup=11
max_connections=100
max_replication_lag=10
},
{
address="mysql-shard2-master"
port=3306
hostgroup=20
max_connections=100
max_replication_lag=10
},
{
address="mysql-shard2-slave"
port=3306
hostgroup=21
max_connections=100
max_replication_lag=10
},
{
address="mysql-shard3-master"
port=3306
hostgroup=30
max_connections=100
max_replication_lag=10
},
{
address="mysql-shard3-slave"
port=3306
hostgroup=31
max_connections=100
max_replication_lag=10
}
)

mysql_query_rules =
(
{
rule_id=1
active=1
match_pattern="^SELECT"
destination_hostgroup=11
apply=1
},
{
rule_id=2
active=1
match_pattern="^INSERT"
destination_hostgroup=10
apply=1
},
{
rule_id=3
active=1
match_pattern="^UPDATE"
destination_hostgroup=10
apply=1
},
{
rule_id=4
active=1
match_pattern="^DELETE"
destination_hostgroup=10
apply=1
}
)

mysql_users =
(
{
username = "app_user"
password = "apppass"
default_hostgroup = 10
active = 1
}
)
